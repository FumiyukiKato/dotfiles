#!/usr/local/bin/bash
DIR="$(pwd)"
VSCODE="$HOME/Library/Application Support/Code/User"

cp $HOME/.zprofile $DIR/zsh/.zprofile

cp $HOME/.zshrc $DIR/zsh/.zshrc

cp $HOME/.bash_profile $DIR/

cp $HOME/.bashrc $DIR/

cp $HOME/.vimrc $DIR/

cp $HOME/.tmux.conf $DIR/

cp $HOME/.emacs.d/init.el $DIR/.emacs.d/

cp "$VSCODE/keybindings.json" $DIR/vscode/
cp "$VSCODE/settings.json" $DIR/vscode/

